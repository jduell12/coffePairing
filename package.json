{
  "name": "coffeepairing",
  "version": "1.0.0",
  "description": "API to keep track of coffee pairing amongst co-workers for a monthly meetup",
  "main": "index.js",
  "scripts": {
    "migrate": "knex migrate:latest --env=development",
    "migrate:test": "knex migrate:latest --env=testing",
